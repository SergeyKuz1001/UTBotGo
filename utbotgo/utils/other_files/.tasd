for *.go_template:
    codeBlock: <=template><=statement><=result><*getterVar>(?s)(\{\{(\w|\.)+\}\})|(\{\{.+?\}\|\{.*?\}\})
    getterVar: (?s)(\{\{<varName>\}\})|(\{\{<template>\}\|\{<separator>\}\})
    template:  <*getterVar>.+?
    separator: .*?
    varName:   <*field>(\w|\.)+
    field:     \.?<fieldName>
    fieldName: \w+
for *.go:
    codeBlock: <=template><=statement><=result><*getterVar>(?s)(\[\[(\w|\.)+\]\])|(\[\[.+?\]\|\[.*?\]\])
    getterVar: (?s)(\[\[<varName>\]\])|(\[\[<template>\]\|\[<separator>\]\])
    template:  <*getterVar>.+?
    separator: .*?
    varName:   <*field>(\w|\.)+
    field:     \.?<fieldName>
    fieldName: \w*
